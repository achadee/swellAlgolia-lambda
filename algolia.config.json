{
  "defaultRanking": ["desc(name)"],
  "rankings": [
    {
      "suffix": "price_desc",
      "ranking": [
        "desc(price)"
      ]
    },
    {
      "suffix": "price_asc",
      "ranking": [
        "desc(price)"
      ]
    }
  ],
  "searchableAttributes": ["name"],
  "attributesForFaceting": [
    "attributes.brand",
    "categories",
    "filterOnly(category_slugs)",
    "attributes.colour",
    "attributes.size",
    "attributes.condition",
    "attributes.material",
    "attributes.vendor"
  ],
  "renderingContent": {
    "facetOrdering": {
      "facets": {
        "order": [
          "categories",
          "attributes.brand",
          "attributes.colour",
          "attributes.size",
          "attributes.condition",
          "attributes.material",
          "attributes.vendor"
        ]
      },
      "values": {
        "attribute.brand": {
          "sortRemainingBy": "alpha"
        }
      }
    }
  }
}
